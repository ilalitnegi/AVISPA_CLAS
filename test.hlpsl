        H1,H2,H3:hash func,
        ID,UID,PID,L,X,PKms,Aa,Bb,Qq,Eata,A,B,Q,PK1,PK2,PIDd,
        PK1d,PK2d,Ts,Msg,Alpha,Beta,Betai,Gamma:text
  init
        State := 0
  transition
        1. State = 0 ^ RCV(Eata.Aa.Bb.X.Qq.Pkms.H1.H2.H3) =|>
        State':=1 ^ RCV(PK1.PK2.PID)
        ^RCV(Alpha.Betai.Msg.Ts)
